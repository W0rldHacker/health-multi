{
  "name": "@worldhacker/health-multi",
  "version": "0.12.1",
  "description": "CLI for parallel polling /health, TUI, aggregated statuses, JSON/Prometheus export, strict exit codes",
  "license": "MIT",
  "type": "commonjs",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "docs",
    "LICENSE"
  ],
  "bin": {
    "health-multi": "./dist/bin.js"
  },
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:binaries": "node ./scripts/build-binaries.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --max-warnings=0 --fix",
    "format": "prettier --check \"**/*.{cjs,cts,js,json,jsx,md,mjs,mts,ts,tsx,yml,yaml}\"",
    "format:write": "prettier --write \"**/*.{cjs,cts,js,json,jsx,md,mjs,mts,ts,tsx,yml,yaml}\"",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "changeset": "changeset",
    "release": "changeset publish",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepack": "npm run build",
    "bench:storage": "tsc --project tsconfig.bench.json && node benchmarks/fix-esm-extensions.mjs && node --enable-source-maps benchmarks/.dist/benchmarks/storage-bench.js",
    "bench:storage:cpu": "tsc --project tsconfig.bench.json && node benchmarks/fix-esm-extensions.mjs && node --enable-source-maps --cpu-prof --cpu-prof-dir=./benchmarks/profiles benchmarks/.dist/benchmarks/storage-bench.js",
    "bench:storage:heap": "tsc --project tsconfig.bench.json && node benchmarks/fix-esm-extensions.mjs && node --enable-source-maps --heap-prof --heap-prof-dir=./benchmarks/profiles benchmarks/.dist/benchmarks/storage-bench.js",
    "bench:storage:soak": "tsc --project tsconfig.bench.json && node benchmarks/fix-esm-extensions.mjs && node --enable-source-maps benchmarks/.dist/benchmarks/storage-soak.js"
  },
  "engines": {
    "node": ">=20.10.0 || >=22.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@worldhacker/starpath": "file:vendor/worldhacker-starpath-0.6.2-F7Ib+1.98 (Polaris).tgz",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "ink": "^6.3.1",
    "ink-testing-library": "^4.0.0",
    "lint-staged": "^16.2.3",
    "pkg": "^5.8.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "tar": "^7.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4",
    "yazl": "^3.3.1"
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}": [
      "prettier --write",
      "eslint --max-warnings=0 --fix"
    ],
    "*.{json,md,yml,yaml}": "prettier --write"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0",
    "p-limit": "^7.1.1",
    "undici": "^7.16.0",
    "yaml": "^2.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "pkg": {
    "assets": [
      "package.json"
    ]
  }
}
